{
  "version": 2,
  "builds": [
    {
      "src": "client/vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/client/src/Utils/Fonts/(.*)",
      "dest": "/client/src/Utils/Fonts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
